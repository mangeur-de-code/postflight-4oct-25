generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Flight {
  id          String   @id @default(uuid())
  date        DateTime
  aircraft    String
  departure   String
  arrival     String
  duration    Float
  landings    Int
  remarks     String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  userId      String
}