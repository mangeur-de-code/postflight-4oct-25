{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "env": {
    "VITE_API_BASE_URL": "@vite_api_base_url",
    "VITE_AUTH_URL": "@vite_auth_url",
    "VITE_BASE44_APP_ID": "@vite_base44_app_id"
  }
}